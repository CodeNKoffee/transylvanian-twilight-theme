		"editorBracketHighlight.foreground6": "#F5C542",
		"editorBracketHighlight.unexpectedBracket.foreground": "#F97070"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8FA1BF"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other",
				"variable.parameter",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#E6EDF3"
			}
		},
		{
			"name": "Keywords and Control Flow",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.other.using",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#B69CFF"
			}
		},
		{
			"name": "Types and Classes",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.name.namespace",
				"entity.other.inherited-class",
				"support.type",
				"support.class",
				"storage.type.primitive",
				"storage.type.built-in"
			],
			"settings": {
				"foreground": "#C4B5FD"
			}
		},
		{
			"name": "Functions and Methods",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#4FC3F7"
			}
		},
		{
			"name": "Constants and Literals",
			"scope": [
				"constant",
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.escape",
				"variable.other.constant",
				"support.constant"
			],
			"settings": {
				"foreground": "#FF6AD5"
			}
		},
		{
			"name": "Numbers and Booleans",
			"scope": [
				"constant.numeric",
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#7FE9FF"
			}
		},
		{
			"name": "Strings and Templates",
			"scope": [
				"string",
				"string.quoted",
				"string.template",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#2EE6A6"
			}
		},
		{
			"name": "Properties and Operators",
			"scope": [
				"variable.other.property",
				"meta.object-literal.key",
				"support.type.property-name",
				"keyword.operator",
				"punctuation",
				"meta.brace",
				"meta.bracket",
				"meta.delimiter"
			],
			"settings": {
				"foreground": "#B8C4D9"
			}
		},
		{
			"name": "Tags (HTML/XML)",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#B69CFF"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#4FC3F7"
			}
		},
		{
			"name": "CSS Selectors",
			"scope": [
				"entity.name.tag.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#B69CFF"
			}
		},
		{
			"name": "CSS Properties",
			"scope": [
				"support.type.property-name.css"
			],
			"settings": {
				"foreground": "#4FC3F7"
			}
		},
		{
			"name": "CSS Values",
			"scope": [
				"support.constant.property-value.css",
				"constant.numeric.css"
			],
			"settings": {
				"foreground": "#2EE6A6"
			}
		},
		{
			"name": "JSON Keys",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#4FC3F7"
			}
		},
		{
			"name": "Markdown Headings",
			"scope": [
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#B69CFF"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E6EDF3"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E6EDF3"
			}
		},
		{
			"name": "Markdown Code",
			"scope": [
				"markup.inline.raw",
				"markup.fenced_code"
			],
			"settings": {
				"foreground": "#2EE6A6"
			}
		},
		{
			"name": "Markdown Links",
			"scope": [
				"markup.underline.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#4FC3F7"
			}
		},
		{
			"name": "Git Added",
			"scope": [
				"markup.inserted.git_gutter"
			],
			"settings": {
				"foreground": "#34D399"
			}
		},
		{
			"name": "Git Modified",
			"scope": [
				"markup.changed.git_gutter"
			],
			"settings": {
				"foreground": "#60A5FA"
			}
		},
		{
			"name": "Git Deleted",
			"scope": [
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#F87171"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#F97070"
			}
		},
		{
			"name": "Warnings",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#F5C542"
			}
		}
	],
	"semanticTokenColors": {
		"variable": "#E6EDF3",
		"variable.readonly": "#E6EDF3",
		"parameter": "#E6EDF3",
		"function": "#4FC3F7",
		"method": "#4FC3F7",
		"class": "#C4B5FD",
		"interface": "#C4B5FD",
		"enum": "#C4B5FD",
		"type": "#C4B5FD",
		"namespace": "#C4B5FD",
		"property": "#B8C4D9",
		"variable.defaultLibrary": "#7FE9FF",
		"function.defaultLibrary": "#4FC3F7",
		"keyword": "#B69CFF"
	}
}
